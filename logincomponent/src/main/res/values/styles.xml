<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="textThemeBold" parent="TextAppearance.AppCompat">
        <item name="android:textColor">#000000</item>
        <item name="android:fontFamily">bold</item>
    </style>

    <style name="textViewThemeBlack" parent="TextAppearance.AppCompat">

        <item name="android:textColor">@android:color/black</item>
        <item name="android:textSize">20sp</item>
    </style>
    <style name="EditTextDefault" parent="android:Widget.EditText">
        <item name="android:background">@android:color/white</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:layout_marginLeft">@dimen/marginEdittextDef</item>
        <item name="android:layout_marginRight">@dimen/marginEdittextDef</item>
        <item name="android:layout_marginTop">@dimen/marginEdittextDef</item>
    </style>
    <style name="ButtonDefault" parent="android:Widget.Button">
        <item name="android:background">@android:color/darker_gray</item>
        <item name="android:textColor">@android:color/black</item>
        <item name="android:radius">50dp</item>
    </style>
</resources>